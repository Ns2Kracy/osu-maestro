{
  "identifier": "main",
  "description": "permissions for desktop app",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "core:event:allow-listen",
    "core:event:default",
    "fs:default",
    {
      "identifier": "fs:allow-exists",
      "allow": [
        {
          "path": "$APPDATA/*"
        }
      ]
    }
  ]
}
