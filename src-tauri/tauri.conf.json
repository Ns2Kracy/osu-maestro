{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "productName": "maestro",
  "identifier": "maestro.ns2kracy.com",
  "build": {
    "beforeDevCommand": "bun dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "bun build",
    "frontendDist": "../.output/public"
  },
  "plugins": {},
  "app": {
    "windows": [
      {
        "title": "maestro",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": false
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.png",
      "icons/icon.ico"
    ]
  }
}
